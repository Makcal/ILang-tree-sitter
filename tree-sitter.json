{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "i_lang",
      "camelcase": "ILang",
      "title": "ILang",
      "scope": "source.i_lang",
      "file-types": [
        "i_lang"
      ],
      "injection-regex": "^i_lang$",
      "class-name": "TreeSitterILang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Project I for Compilers Construction course",
    "authors": [
      {
        "name": "Maxim Fomin",
        "email": "m.fomin@innopolis.university",
        "url": "https://github.com/Makcal"
      }
    ],
    "links": {
      "repository": "https://github.com/Makcal/ILang-tree-sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}